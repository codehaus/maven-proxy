<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>maven-proxy-webapp</display-name>
    <description>
		Maven Repository Proxy Webapp $Name$
    </description>
	
	<servlet>
            <servlet-name>RepositoryServlet</servlet-name>
            <servlet-class>org.apache.maven.proxy.RepositoryServlet</servlet-class>
    </servlet>
    
    <servlet>
            <servlet-name>ResourceServlet</servlet-name>
            <servlet-class>org.apache.maven.proxy.ResourceServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
      <servlet-name>RepositoryServlet</servlet-name>
      <url-pattern>/repository/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
      <servlet-name>ResourceServlet</servlet-name>
      <url-pattern>/images/*</url-pattern>
      <url-pattern>/styles/*</url-pattern>
    </servlet-mapping>
       
</web-app>