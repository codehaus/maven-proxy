<?xml version="1.0"?>
<project>
  <extend>../core/base-project.xml</extend>
  <pomVersion>3</pomVersion>
  <id>maven-proxy-standalone</id>
  <groupId>maven</groupId>
  <name>Maven Repository Proxy - Standalone Project</name>
  <package>org.apache.maven.proxy.standalone</package>
  <description>Simulates a remote repository that has upstream providers.
   You download from it, it downloads from parent.  Avoids full mirroring.
  </description>

  <dependencies>
    <dependency>
      <id>maven:maven-proxy-core</id>
      <version>SNAPSHOT</version>
    </dependency>
    
    <dependency>
      <id>avalon-framework</id>
      <version>4.1.4</version>
      <properties><war.bundle>true</war.bundle></properties>
    </dependency>
    
    <dependency>
      <id>servletapi</id>
      <version>2.3</version>
    </dependency>
    
    <dependency>
      <id>jetty</id>
      <version>4.2.9</version>
    </dependency>

	<dependency>
      <id>log4j</id>
      <version>1.2.8</version>
    </dependency>

    <dependency>
      <id>maven:maven-fetch</id>
      <version>SNAPSHOT</version>
    </dependency>
  </dependencies>

  
</project>

